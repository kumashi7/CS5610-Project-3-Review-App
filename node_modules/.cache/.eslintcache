        <form onSubmit={(event) => updateEntryAxios(id, title, genre, \n                release, content, event)}>\n                <label for=\"title\">title:</label>\n                <input value={title} onChange={(e) => setTitle(e.target.value)} \n                    type=\"text\" id=\"title\" name=\"title\"/>\n                <label for=\"genre\">genre:</label>\n                <input type=\"text\" id=\"genre\" name=\"genre\" value={genre} \n                    onChange={(e) => setGenre(e.target.value)} />\n                <label for=\"release\">release:</label>\n                <input type=\"number\" id=\"release\" name=\"release\" value={release} \n                    onChange={(e) => setRelease(e.target.value)} />\n                <label for=\"content\">content:</label>\n                <input type=\"text\" id=\"content\" name=\"content\" value={content}\n                    onChange={(e) => setContent(e.target.value)} />\n                <input type=\"submit\" value=\"Submit\"/>\n                <div>{id}</div>\n            </form>\n        </div>\n    )\n\n}",[],"/Users/ying/Desktop/NEU/2022Spring/CS5610/Project/CS5610-Project-3-Review-App/src/components/login/Login.jsx",[],[],"/Users/ying/Desktop/NEU/2022Spring/CS5610/Project/CS5610-Project-3-Review-App/src/components/createUser/createUser.jsx",[],[],"/Users/ying/Desktop/NEU/2022Spring/CS5610/Project/CS5610-Project-3-Review-App/src/components/entry/EntryCard.jsx",["66","67","68"],[],"import React, { useState, useEffect } from 'react'\nimport { Link } from 'react-router-dom';\n\nimport './Entry.css'\n\n// Entry card item\nfunction EntryCard({list}) {\n  const [edit, setEdit] = useState ({\n    id: null,\n    value: ''\n  })\n\n  // useEffect(() => {\n  //   console.log(list);\n  // },[]);\n\n  return list.map(({title, genre, release, content, reviews, _id}) => \n  <Link to=\"/entryDetails\" state ={{id: _id}}>\n    <div key={_id} className='container flex'>\n      <h2>{title}</h2>\n      <hr></hr>\n      <h